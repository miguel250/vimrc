{
  "enabled": true,
  "pyls": {
    "configurationSources": ["flake8"],
    "plugins": {
      "flake8": {
        "enabled": true
      },
      "pyls_mypy": {
          "enabled": true,
          "live_mode": true
      },
      "pycodestyle": {
        "enabled": false
      },
      "pyflakes": {
        "enabled": true
      },
      "pylint": {
          "enabled": true
      }
    }
  },
  "gopls": {
    "completeUnimported": true,
    "staticcheck": true,
    "analyses": {
      "unreachable": true
    }
  }
}

